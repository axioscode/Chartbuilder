@font-face {
    font-family: 'Gordita-Light';
    src: url('https://static.axios.com/fonts/gorditalight-webfont.eot');
    src: url('https://static.axios.com/fonts/gorditalight-webfont.eot?#iefix') format('embedded-opentype'),
         url('https://static.axios.com/fonts/gorditalight-webfont.woff2') format('woff2'),
         url('https://static.axios.com/fonts/gorditalight-webfont.woff') format('woff'),
         url('https://static.axios.com/fonts/gorditalight-webfont.ttf') format('truetype'),
         url('https://static.axios.com/fonts/gorditalight-webfont.svg#gorditalight') format('svg');
    font-weight: 200;
    font-style: normal;
    unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2212, U+2215, U+E0FF, U+EFFD, U+F000;
}

